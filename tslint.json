{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single"],
    "prettier": true,
    "no-var-requires": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["*.json", "**/*.json"],
    "format": "verbose"
  }
}
